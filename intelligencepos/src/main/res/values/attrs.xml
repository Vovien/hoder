<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="IncludeTitle">
        <attr name="title" format="string"/>
        <attr name="returnVisibility">
            <enum name="visibility" value="1"/>
            <enum name="gone" value="-1"/>
        </attr>
        <attr name="returnRes" format="reference"/>
        <attr name="menuResVisibility">
            <enum name="visibility" value="1"/>
            <enum name="gone" value="-1"/>
        </attr>
        <attr name="menuRes" format="reference"/>
        <attr name="menuTextVisibility">
            <enum name="visibility" value="1"/>
            <enum name="gone" value="-1"/>
        </attr>
        <attr name="menuText" format="string"/>
    </declare-styleable>

    <declare-styleable name="CircleProgress">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="progressTextColor" format="color"/>
        <attr name="progressTextSize" format="dimension"/>
        <attr name="identifyTextColor" format="color"/>
        <attr name="identifyTextSize" format="dimension"/>
        <attr name="percentTextColor" format="color"/>
        <attr name="percentTextSize" format="dimension"/>
        <attr name="marginBetweenProgressAndPercent" format="dimension"/>
        <attr name="marginBetweenProgressAndIdentify" format="dimension"/>
    </declare-styleable>

</resources>